{
  "contentTypeUri": "https://sfcc.com/components/features",
  "status": "ACTIVE",
  "settings": {
    "label": "Features",
    "icons": [
      {
        "size": 256,
        "url": "https://sfcccomposable.a.bigcontent.io/v1/static/icon-component-features"
      }
    ],
    "visualizations": [
      {{#each visualisations}}
      {
        "label": "{{this.name}}",
        "templatedUri": "{{this.url}}/\{{locales}}/visualization?vse=\{{vse.domain}}&contentId=\{{content.sys.id}}&hub=\{{hub.name}}",
        "default": "{{this.default}}"
      },
      {{/each}}
      {
        "label": "JSON",
        "templatedUri": "https://delivery-preview.visualizations.content.amplience.net?id=\{{content.sys.id}}&vse=\{{vse.domain}}&locale=\{{locales}}&hub=\{{cms.hub}}&realtime=true",
        "default": false
      }
    ],
    "cards": [
      {
        "templatedUri": "https://bigcontent.io/cms/cards/gallery/index.html?image0=/features/0/icon&image1=/features/1/icon&image2=/features/2/icon&image3=/features/3/icon&imageAlt0=/features/0/title/values/0/value&imageAlt1=/features/1/title/values/0/value&imageAlt2=/features/2/title/values/0/value&imageAlt3=/features/3/title/values/0/value&src=%2F%2F\{{vse.domain}}%2Fcms%2Fcontent%2Fquery%3FfullBodyObject%3Dtrue%26query%3D%257B%2522sys.iri%2522%253A%2522http%253A%252F%252Fcontent.cms.amplience.com%252F\{{content.sys.id}}%2522%257D%26scope%3Dtree%26store%3Dstaging"
      }
    ]
  },
  "repositories": [
    "content"
  ]
}